<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        <nav class="navbar">
            <ul class="list-group">
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Schools/List">Schools</a>
                </li>
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Floors/List">Floors</a>
                </li>
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Rooms/List">Rooms</a>
                </li>
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Employees/List">Employees</a>
                </li>
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Students/List">Students</a>
                </li>
                <li class="list-group-item list-group-item-success">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Positions/List">Positions</a>
                </li>
            </ul>
        </nav>
    </header>
</body>
@section Styles
