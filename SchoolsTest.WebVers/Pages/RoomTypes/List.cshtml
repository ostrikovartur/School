@page "/roomTypes"
@model SchoolsTest.WebVers.Pages.RoomTypes.List
@{
}
<h1>List room types</h1>
<h3><a asp-page="../RoomTypes/Add">Add room type</a></h3>
<table align="left" width="500px" border="1">
    @foreach (var roomType in Model.RoomTypes)
    {
        <tr>
            <td>@roomType.Id</td>
            <td>|</td>
