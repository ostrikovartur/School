            <td>|</td>
            @*            <td><a asp-page="Floor" asp-route-id="@position.Id">Select position</a></td>
        <td>|</td>*@
            <td><a asp-page="Edit" asp-route-id="@roomType.Id">Edit room type</a></td>
        </tr>
    }
</table>