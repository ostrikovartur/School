@page "/positions"
@model SchoolsTest.WebVers.Pages.Positions.List
@{
}
<h1>List positions</h1>
<h3><a asp-page="../Positions/Add">Add position</a></h3>
<table align="left" width="500px" border="1">
    @foreach (var position in Model.Positions)
    {
        <tr>
            <td>@position.Id</td>
            <td>|</td>
            <td>@position.Name</td>
            <td>|</td>
@*            <td><a asp-page="Floor" asp-route-id="@position.Id">Select position</a></td>
            <td>|</td>*@
            <td><a asp-page="Edit" asp-route-id="@position.Id">Edit position</a></td>
        </tr>
    }
</table>