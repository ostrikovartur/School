@page "/{floorId}/rooms"
@model RoomsList
@{
}
<h1>List rooms</h1>
<table align="left" width="300px" border="1">
    @foreach (var room in Model.Rooms)
    {
        <tr>
            <td>@room.Id</td>
            <td>|</td>
            <td>@room.Number</td>
            <td>|</td>
@*            <td>@room.Floor.Number</td>
            <td>|</td>*@
            <td>@room.Type</td>
            <td>|</td>
            <td><a asp-page="Edit" asp-route-id="@room.Id">Edit</a></td>
        </tr>
    }
</table>